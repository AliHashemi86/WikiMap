<!DOCTYPE html>
<html lang="en">
  <head>
    <title>Home Page</title>
  </head>

  <body>
    <!-- header partial -->
    <header><%- include('partials/_header')%></header>
    <main>
      <!-- **** Featured Maps Section Begins ****  -->

      <!-- Featured Map 1 -->
      <section class="featured-maps">
        <article class="feature-map-1">
          <li>
            <a href="/map link"></a>
            <h1>Map 1</h1>
            <p>Text description about map 1</p>
            <img src="/" alt="Map 1" />
          </li>
        </article>
        <!-- Featured Map 2 -->
        <div class="feature-row">
          <article class="feature-map-2">
            <a href="/map2"></a>
            <h1>Map 2</h1>
            <p>Text description about map 2</p>
            <img src="/" alt="Map 2" />
          </article>

          <!-- Featured Map 3 -->
          <article class="feature-map-3">
            <a href="/map3"></a>
            <h1>Map 3</h1>
            <p>Text description about map 3</p>
            <img src="/" alt="Map 3" />
          </article>
        </div>
      </section>
      <!-- **** Featured Maps Section Ends **** -->

      <!-- **** MapApp Features/'Call to action' Section Begins ****  -->
      <!-- MapApp Features, Section 1/2  -->
      <section class="mapapp-feature1">
        <div class="feature-text">
          <li>
            <h1>Feature 1</h1>
            <h4>Subheader of F1</h4>
            <p>Description of F1</p>
          </li>
        </div>
        <div class="feature-img">
          <img src="/" alt="Cool illustration of F1" />
        </div>
      </section>
      <!-- MapApp Features, Section 2/2  -->
      <section class="mapapp-feature2">
        <div class="feature-text">
          <li>
            <h1>F2</h1>
            <h4>F2</h4>
            <p>F2</p>
          </li>
        </div>
        <div class="feature-img">
          <img src="/" alt="Cool illustration of F2" />
        </div>
      </section>
      <!-- MapApp Call To Action -->
      <section class="c2a">
        <div>
          <article>
            <div>
              <h1>CTA</h1>
              <h4>Subheader of CTA</h4>
              <p>Description of CTA</p>
              <button>CTA</button>
            </div>
          </article>
        </div>
      </section>
      <!-- **** MapApp Feature Section Ends **** -->

      <!-- Footer partial -->
      <footer><%- include('partials/_footer')%></footer>
    </main>
  </body>
</html>
