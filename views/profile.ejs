                </div>
                <div>
                  <input type="text" placeholder="Username" value="*" />
                </div>
                <div>
                  <input type="text" placeholder="Email" value="*" />
                </div>
                <div>
                  <input type="text" placeholder="Password" value="*" />
                </div>
                <div>
                  <button type="button">Save</button>
                </div>
              </section>
            </div>

        <!-- Favorites Section -->
        <section>
          <!-- Container - Outer Box -->
          <div>
            <!-- Container - Inner Box -->
            <div>
              <a href="">MapName</a>
              <img src="/" alt="Map Thumbnail">
            </div>
          </div>
        </section>
    </main>

    <!-- footer start -->
    <footer><%- include('partials/_footer')%></footer>
    <!-- footer end -->
  </body>
